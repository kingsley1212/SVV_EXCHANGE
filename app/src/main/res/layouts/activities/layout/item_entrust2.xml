<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:background="@color/c_white"
    android:layout_height="wrap_content">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="134dp"
        android:orientation="vertical">
        <RelativeLayout
            android:layout_marginLeft="10dp"
            android:layout_marginTop="15dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="bottom">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                >
                <ImageView
                    android:visibility="gone"
                    android:layout_width="21dp"
                    android:layout_height="21dp"
                    android:src="@mipmap/coin1"/>
                <TextView
                    android:id="@+id/tv_entrust_type"
                    android:gravity="center"
                    android:textSize="14sp"
                    android:layout_gravity="center"
                    android:text="@string/countdetailact_btn2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@drawable/buy_sell_type_selector"/>
                <TextView
                    android:id="@+id/tv_market1"
                    android:textSize="14sp"
                    android:layout_marginLeft="4dp"
                    android:layout_gravity="center"
                    android:text="ETH"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/c_2183c9"/>
                <ImageView
                    android:layout_gravity="center"
                    android:layout_marginLeft="4dp"
                    android:layout_width="18dp"
                    android:layout_height="18dp"
                    android:src="@mipmap/arrow"/>
                <ImageView
                    android:visibility="gone"
                    android:layout_marginLeft="5dp"
                    android:layout_width="21dp"
                    android:layout_height="21dp"
                    android:src="@mipmap/coin2"/>
                <TextView
                    android:id="@+id/tv_market2"
                    android:textSize="13sp"
                    android:layout_marginLeft="4dp"
                    android:layout_gravity="center"
                    android:text="ETH"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/c_2183c9"/>

            </LinearLayout>
            <TextView
                android:id="@+id/tv_entrust_status"
                android:layout_marginRight="10dp"
                android:textColor="@color/c_1a456d"
                android:layout_alignParentRight="true"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="15sp"
                android:text="@string/barbusinessfragment_transaction_status"/>
        </RelativeLayout>
        <RelativeLayout
            android:layout_marginTop="15dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:textColor="@color/tag_tv"
                android:id="@+id/tv_entrust_price"
                android:layout_marginLeft="10dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="12sp"
                android:text="@string/barbusinessfragment_price1"/>
            <TextView
                android:textColor="@color/tag_tv"
                android:id="@+id/tv_entrust_num"
                android:layout_alignParentRight="true"
                android:layout_marginRight="10dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="12sp"
                android:text="@string/barbusinessfragment_price1"/>
        </RelativeLayout>
        <RelativeLayout
            android:layout_marginTop="10dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:textColor="@color/tag_tv"
               android:id="@+id/tv_entrust_total"
                android:layout_marginLeft="10dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="12sp"
                android:text="@string/barbusinessfragment_price1"/>
            <TextView
                android:textColor="@color/tag_tv"
                android:id="@+id/tv_entrust_yijiaoyi"
                android:layout_alignParentRight="true"
                android:layout_marginRight="10dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="12sp"
                android:text="@string/barbusinessfragment_price1"/>
        </RelativeLayout>
        <RelativeLayout
            android:layout_marginTop="10dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:textColor="@color/tag_tv"
                android:id="@+id/tv_entrust_time"
                android:layout_marginLeft="10dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="12sp"
                android:text="@string/barbusinessfragment_price1"/>
            <TextView
                android:background="@drawable/btn_universal"
                android:gravity="center"
                android:textColor="@color/c_white"
                android:id="@+id/tv_entrust_undo"
                android:layout_alignParentRight="true"
                android:layout_marginRight="10dp"
                android:layout_width="55dp"
                android:layout_height="20dp"
                android:textSize="12sp"
                android:text="@string/entrust_chexiao"
                android:visibility="gone"/>
            <TextView
                android:id="@+id/iv_comlete"
                android:background="@drawable/btn_9fb8cf"
                android:gravity="center"
                android:textColor="@color/c_white"
                android:layout_alignParentRight="true"
                android:layout_marginRight="10dp"
                android:layout_width="55dp"
                android:layout_height="20dp"
                android:textSize="12sp"
                android:text="@string/business_trade_yet"/>
        </RelativeLayout>

    </LinearLayout>

   <View
       android:layout_width="match_parent"
       android:layout_height="1px"
       android:background="@color/line_color1"
       android:layout_alignParentBottom="true"/>

    
</RelativeLayout>
