<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:layout_width="match_parent"
              android:layout_height="wrap_content"
              android:orientation="vertical">


    <LinearLayout
        android:layout_width="265dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_margin="@dimen/marginlv1_commsize"
        android:background="@drawable/popup_businesssubmit"
        android:orientation="vertical">


        <TextView
            android:id="@+id/textView5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="27dp"
            android:gravity="center"
            android:text="@string/addpay_bank_psw"
            android:textColor="@color/c_282828"
            android:textSize="16sp"/>
            <EditText
                android:hint="@string/AddorEditBankAct_pw_hint"
                android:id="@+id/aqpass_et"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_toRightOf="@id/aqpass_tx"
                android:layout_alignParentRight="true"
                android:layout_marginLeft="10dp"
                android:background="@null"
                android:inputType="textPassword"
                android:layout_centerVertical="true"
                android:layout_marginRight="@dimen/marginlv1_commsize"
                android:textColor="@color/c_282828"
                android:maxLines="1"
                android:ellipsize="end"
                android:textSize="13sp"/>


        <View
            style="@style/divider_line_horizontal_nocolor"
            android:background="@color/c_dddddd"/>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/cancle"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center"
                android:text="@string/dialog_cancel"
                android:textColor="@color/c_2459e5"
                android:textSize="14sp"/>

            <View
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:background="@color/c_dddddd"/>


            <TextView
                android:id="@+id/sure"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center"
                android:text="@string/dialog_commit"
                android:textColor="@color/c_2459e5"
                android:textSize="14sp"/>

        </LinearLayout>

    </LinearLayout>


</LinearLayout>