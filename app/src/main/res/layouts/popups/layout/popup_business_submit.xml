<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:layout_width="match_parent"
              android:layout_height="wrap_content"
              android:orientation="vertical">


    <LinearLayout
        android:layout_width="265dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_margin="@dimen/marginlv1_commsize"
        android:background="@drawable/popup_businesssubmit"
        android:orientation="vertical">


        <TextView
            android:id="@+id/textView5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="27dp"
            android:gravity="center"
            android:text="@string/popupBusinessSubmit_title"
            android:textColor="@color/c_282828"
            android:textSize="16sp"/>


        <RelativeLayout
            android:id="@+id/price_rl"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="15dp">

            <TextView
                android:id="@+id/price_tv_1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_centerVertical="true"
                android:layout_marginLeft="15dp"
                android:textColor="@color/c_282828"
                android:textSize="12sp"/>


            <TextView
                android:id="@+id/price_tx"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_toRightOf="@id/price_tv_1"
                android:layout_marginRight="@dimen/marginlv1_commsize"
                android:layout_marginLeft="30dp"
                android:gravity="right"
                android:maxLines="1"
                android:ellipsize="end"
                android:textColor="@color/c_282828"
                android:textSize="13sp"

                />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/num_rl"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="13dp">

            <TextView
                android:id="@+id/num_tv_1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_centerVertical="true"
                android:layout_marginLeft="15dp"
                android:textColor="@color/c_282828"
                android:textSize="12sp"/>


            <TextView
                android:id="@+id/number_tx"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_toRightOf="@id/num_tv_1"
                android:layout_marginRight="@dimen/marginlv1_commsize"
                android:layout_marginLeft="30dp"
                android:gravity="right"
                android:maxLines="1"
                android:ellipsize="end"
                android:textColor="@color/c_282828"
                android:textSize="13sp"/>
        </RelativeLayout>


        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginTop="13dp">

            <TextView
                android:id="@+id/aqpass_tx"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_centerVertical="true"
                android:layout_marginLeft="16dp"
                android:text="@string/popupBusinessSubmit_tx4"
                android:textColor="@color/c_282828"
                android:textSize="12sp"/>


            <EditText
                android:hint="@string/AddorEditBankAct_pw_hint"
                android:id="@+id/aqpass_et"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_toRightOf="@id/aqpass_tx"
                android:layout_alignParentRight="true"
                android:layout_marginLeft="10dp"
                android:background="@null"
                android:inputType="textPassword"
                android:layout_centerVertical="true"
                android:gravity="right|center_vertical"
                android:layout_marginRight="@dimen/marginlv1_commsize"
                android:textColor="@color/c_282828"
                android:maxLines="1"
                android:ellipsize="end"
                android:textSize="13sp"/>
        </RelativeLayout>


        <View
            style="@style/divider_line_horizontal_nocolor"
            android:background="@color/c_dddddd"/>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/cancle"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center"
                android:text="@string/popupBusinessSubmit_cancle"
                android:textColor="@color/c_2459e5"
                android:textSize="14sp"/>

            <View
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:background="@color/c_dddddd"/>


            <TextView
                android:id="@+id/sure"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center"
                android:text="@string/popupBusinessSubmit_sure"
                android:textColor="@color/c_2459e5"
                android:textSize="14sp"/>

        </LinearLayout>

    </LinearLayout>


</LinearLayout>